{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/autoMapItems",
        "function": "autoMapItems"
      },
      {
        "source": "/api/gemini",
        "function": "gemini"
      },
      {
        "source": "/api/correctTypos",
        "function": "correctTypos"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
